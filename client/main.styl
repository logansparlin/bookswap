dark-blue = #1c296a
light-blue = #3a488c

* 
	font-family 'Lato'
	font-weight normal
	-webkit-font-smoothing antialiased

html, body 
	height 100%
	margin 0
	padding 0
	overflow-x hidden
	overflow-y scroll
	-webkit-overflow-scrolling touch
h1, h2, h3, h4, h5, h6, p, span, div, ul 
	padding 0
	margin 0

@import 'nib'
@import 'jeet'
@import 'rupture'
@import 'views/**/*.import.styl'